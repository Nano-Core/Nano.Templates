{
  "App": {
    "Name": "Advanced",
    "Description": "Advanced service implementation.",
    "Version": "1.0.0",
    "TermsOfService": null,
    "Contact": {
      "Name": "community contributors",
      "Email": "support@nano.com",
      "Url": "http://www.nano.com"
    },
    "License": {
      "Name": "The Unlicense",
      "Url": "http://unlicense.org"
    },
    "Hosting": {
      "Root": "api",
      "Ports": [
        80,
        5000
      ]
    },
    "Cultures": {
      "Default": "en-US",
      "Supported": [
        "en-US"
      ]
    }
  },
  "Data": {
    "BatchSize": "25",
    "BulkBatchSize": "500",
    "BulkBatchDelay": "1000",
    "UseAudit": true,
    "UseMemoryCache": true,
    "UseSoftDeletetion": true,
    "ConnectionString": "Server=mysql;Database=advancedDb;Uid=myUser;Pwd=myPassword"
  },
  "Logging": {
    "LogLevel": "Debug",
    "Sinks": [
      "Console"
    ],
    "LogLevelOverrides": [
      {
        "Namespace": "System",
        "LogLevel": "Debug"
      }
    ]
  },
  "Eventing": {
    "Host": "rabbitmq",
    "VHost": "/",
    "Port": 5672,
    "AuthenticationCredential": {
      "Username": "myUser",
      "Password": "myPassword"
    },
    "UseSsl": false,
    "Timeout": 30,
    "Heartbeat": 10
  }
}