    "LogLevelOverrides": [
      {
        "Namespace": "System",
        "LogLevel": "Information"
      },
      {
        "Namespace": "Microsoft",
        "LogLevel": "Warning"
      },
      {
        "Namespace": "AspNetCore.HealthChecks",
        "LogLevel": "Warning"
      },
      {
        "Namespace": "HealthChecks.UI",
        "LogLevel": "Warning"
      }
