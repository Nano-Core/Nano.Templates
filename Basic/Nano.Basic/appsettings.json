{
  "App": {
    "Name": "Basic",
    "Description": null,
    "Version": "1.0.0.0",
    "TermsOfService": null,
    "Cultures": {
      "Default": "en-US",
      "Supported": [
        "en-US"
      ]
    }
  },
  "Web": {
    "Hosting": {
      "Root": "api",
      "Ports": [
        80
      ],
      "UseSsl": false
    },
    "Documentation": {
      "Contact": {
        "Name": null,
        "Email": null,
        "Url": null
      },
      "License": null
    }
  },
  "Data": {
    "BatchSize": 25,
    "BulkBatchSize": 500,
    "BulkBatchDelay": 1000,
    "QueryIncludeDepth": 4,
    "UseAudit": true,
    "UseMemoryCache": true,
    "UseLazyLoading": true,
    "UseCreateDatabase": false,
    "UseMigrateDatabase": true,
    "UseSoftDeletetion": true,
    "ConnectionString": "Server=mysql;Database=basicDb;Uid=myUser;Pwd=myPassword"
  },
  "Security": {
    "IsEnabled": true,
    "Jwt": {
      "Issuer": "development.api.basic",
      "Audience": "development.api.basic",
      "SecretKey": "development.secretkey",
      "ExpirationInHours": 72
    },
    "User": {
      "AdminUsername": "admin",
      "AdminPassword": "abc12|+d34DadD",
      "AdminEmailAddress": null,
      "RequireUniqueEmail": true,
      "AllowedUserNameCharacters": null
    },
    "SignIn": {
      "RequireConfirmedEmail": false,
      "RequireConfirmedPhoneNumber": false
    },
    "Lockout": {
      "AllowedForNewUsers": true,
      "MaxFailedAccessAttempts": 3,
      "DefaultLockoutTimeSpan": "00:30:00"
    },
    "Password": {
      "RequireDigit": true,
      "RequireNonAlphanumeric": true,
      "RequireLowercase": true,
      "RequireUppercase": true,
      "RequiredLength": 12,
      "RequiredUniqueCharecters": 3
    }
  },
  "Logging": {
    "Sinks": [
      {
        "Name": "Console"
      }
    ],
    "LogLevel": "Debug"
  },
  "Eventing": {
    "Host": "rabbitmq",
    "VHost": "/",
    "Port": 5672,
    "Username": "myUser",
    "Password": "myPassword",
    "UseSsl": false,
    "Timeout": 30,
    "Heartbeat": 10
  }
}