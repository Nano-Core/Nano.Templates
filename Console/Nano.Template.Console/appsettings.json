{
  "App": {
    "Name": "Console",
    "EntryPoint": "Nano.Template.Console.dll", 
    "Description": "Nano template console template.",
    "Version": "1.0.0.0",
    "TermsOfService": null,
    "Cultures": {
      "Default": "en-US",
      "Supported": [
        "en-US"
      ]
    }
  },
  "Data": {
    "BatchSize": 25,
    "BulkBatchSize": 500,
    "BulkBatchDelay": 1000,
    "QueryRetryCount": 0,
    "QueryIncludeDepth": 4,
    "UseAudit": true,
    "UseAutoSave": false,
    "UseMemoryCache": true,
    "UseLazyLoading": true,
    "UseCreateDatabase": false,
    "UseMigrateDatabase": false,
    "UseSoftDeletetion": true,
    "UseSensitiveDataLogging": false,
    "UseHealthCheck": true,
    "DefaultCollation": null,
    "ConnectionString": null,
    "MemoryCache": {
      "MaxEntries": 5000,
      "ExpirationTimeoutInSeconds": 300,
      "ExpirationScanFrequencyInSeconds": 60,
      "ExpirationMode": "Sliding",
      "IgnoredTableNames": [
      ]
    }
  },
  "Logging": {
    "LogLevel": "Debug"
  },
  "Eventing": {
    "Host": "eventing",
    "VHost": "/",
    "Port": 5672,
    "Username": null,
    "Password": null,
    "UseSsl": false,
    "Timeout": 30,
    "Heartbeat": 60,
    "PrefetchCount": 50,
    "UseHealthCheck": true
  }
}